################################################################################
.PHONY: all
all: install

################################################################################
.PHONY: install
install:
	cp -f .bash_aliases ~/.bash_aliases
	cp -f .bash_export ~/.bash_export
	cp -f .bashrc ~/.bashrc
	cp -f .english.txt ~/.english.txt
	cp -f .sholde ~/.sholde
	cp -f .makefile.template ~/.makefile.template

################################################################################
.PHONY: clean
clean:
	rm -rf *~ .*~
