[user]
        name = Sholde
        username = Sholde
        email =
[init]
        defaultBranch = master
[core]
        editor = emacs -nw
        whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
[alias]
        a = add
        #############
        ama = am --abort
        amr = am --resolved
        ams = am --skip
        am3 = am --3way
        #############
        b = branch
        br = branch
        #############
        ci = commit
        #############
        cl = clone
        #############
        cp = cherry-pick
        #############
        d = diff
        deol = diff --ignore-space-at-eol
        #############
        f = fetch
        #############
        fp = format-patch
        #############
        l = log --oneline
        lg = log --oneline --graph --decorate
        #############
        m = merge
        ma = merge --abort
        mc = merge --continue
        ms = merge --skip
        #############
        co = checkout
        com = checkout master
        cob = checkout -b
        #############
        ps = push
        pst = push --tags
        pso = push origin
        psom = push origin master
        #############
        pl = pull
        plo = pull origin
        plom = pull origin master
        #############
        rb = rebase
        #############
        re = reset
        rh = reset HEAD
        #############
        r = remote
        ra = remote add
        rao = remote add origin
        rro = remote remove origin
        rso = remote show origin
        #############
        s = status
        st = status
        #############
        t = tag
        #############
        w = show
